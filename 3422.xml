<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
	<Object ObjectType="MODefinition">
		<Name>Three-phase electrical meter complement</Name>
		<Description1>The uCIFI three-phase electrical meter complement gathers all the measures, events, threshold and other resources required to create a full three-phase electrical meter together with three additional uCIFI single-phase electrical meters.</Description1>
		<ObjectID>3422</ObjectID>
		<ObjectURN>urn:oma:lwm2m:ext:3422</ObjectURN>
		<LWM2MVersion>1.0</LWM2MVersion>
		<ObjectVersion>1.0</ObjectVersion>
		<MultipleInstances>Multiple</MultipleInstances>
		<Mandatory>Optional</Mandatory>
		<Resources>
			<Item ID="1">
				<Name>L1-L2 Voltage</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>V</Units>
				<Description>Voltage measured between line 1 and line 2.</Description>
			</Item>
			<Item ID="2">
				<Name>L2-L3 Voltage</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>V</Units>
				<Description>Voltage measured between line 2 and line 3.</Description>
			</Item>
			<Item ID="3">
				<Name>L3-L1 Voltage</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>V</Units>
				<Description>Voltage measured between line 3 and line 1.</Description>
			</Item>
			<Item ID="4">
				<Name>Total Current of all three phases</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>A</Units>
				<Description>Sum of current measured on all three lines.</Description>
			</Item>
			<Item ID="5">
				<Name>Instantaneous Power Factor overall</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>Instantaneous power factor overall.</Description>
			</Item>
			<Item ID="6">
				<Name>Last Average Power Factor</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>Last average power factor calculated from each power factor on the 3 lines.</Description>
			</Item>
			<Item ID="7">
				<Name>Minimum Power Factor</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>Minimum power factor measured on the 3 lines.</Description>
			</Item>
			<Item ID="8">
				<Name>System kW: Inst. Value. ΣLi Active power+ (QI+QIV)</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>W</Units>
				<Description>Total active power import on the 3 lines.</Description>
			</Item>
			<Item ID="9">
				<Name>System kW: Inst. Value. ΣLi Active power- (QII+QIII)</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>W</Units>
				<Description>Total active power export on the 3 lines.</Description>
			</Item>
			<Item ID="10">
				<Name>System kvar: Inst. Value. ΣLi Reactive power+ (QI+QII)</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>var</Units>
				<Description>Total reactive power import on the 3 lines.</Description>
			</Item>
			<Item ID="11">
				<Name>System kvar: Inst. Value. ΣLi Reactive power- (QIII+QIV)</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>var</Units>
				<Description>Total reactive power export on the 3 lines.</Description>
			</Item>
			<Item ID="12">
				<Name>Instantaneous Apparent Power import (+VA)</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration>-1..1</RangeEnumeration>
				<Units>VA</Units>
				<Description>Instantaneous apparent power import on the 3 lines.</Description>
			</Item>
			<Item ID="13">
				<Name>Instantaneous Apparent Power export (-VA)</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>VA</Units>
				<Description>Instantaneous apparent power export on the 3 lines.</Description>
			</Item>
			<Item ID="14">
				<Name>Instantaneous Active Power (|+A|+|-A|)</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>W</Units>
				<Description>Instantaneous active power on the 3 lines.</Description>
			</Item>
			<Item ID="15">
				<Name>Instantaneous Net Active Power (|+A|-|-A|)</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>W</Units>
				<Description>Instantaneous net active power on the 3 lines.</Description>
			</Item>
			<Item ID="16">
				<Name>Measurement period of Instantaneous value</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>s</Units>
				<Description>Measurement period of instantaneous value.</Description>
			</Item>
			<Item ID="17">
				<Name>Active energy import (+A) Un</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>Wh</Units>
				<Description>Active energy import (unified rate) on the 3 lines.</Description>
			</Item>
			<Item ID="18">
				<Name>Active energy export (-A) Un</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration />
				<Units>Wh</Units>
				<Description>Active energy export (unified rate) on the 3 lines.</Description>
			</Item>
			<Item ID="19">
				<Name>Active energy (|+A|+|-A|) Combined total</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>Wh</Units>
				<Description>Active energy, combined total, import plus export.</Description>
			</Item>
			<Item ID="20">
				<Name>Active energy (|+A|-|-A|) Combined total</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>Wh</Units>
				<Description>Active energy, combined total, import minus export, on the 3 lines.</Description>
			</Item>
			<Item ID="21">
				<Name>Reactive energy import (+R) Un</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration />
				<Units>varh</Units>
				<Description>Reactive energy import, unified rate, on the 3 lines.</Description>
			</Item>
			<Item ID="22">
				<Name>Reactive energy export (-R) Un</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>varh</Units>
				<Description>Reactive energy export, unified rate, on the 3 lines.</Description>
			</Item>
			<Item ID="23">
				<Name>Reactive energy QI (+Ri) Un</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration />
				<Units>varh</Units>
				<Description>Reactive energy QI (+Ri), unified rate, on the 3 lines.</Description>
			</Item>
			<Item ID="24">
				<Name>Reactive energy QII (+Rc) Un</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>varh</Units>
				<Description>Reactive energy QII (+Rc), unified rate, on the 3 lines.</Description>
			</Item>
			<Item ID="25">
				<Name>Reactive energy QIII (-Ri) Un</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>varh</Units>
				<Description>Reactive energy QIII (-Ri) , unified rate, on the 3 lines.</Description>
			</Item>
			<Item ID="26">
				<Name>Reactive energy QIV (-Rc) Un</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>varh</Units>
				<Description>Reactive energy QIV (-Rc) , unified rate, on the 3 lines.</Description>
			</Item>
			<Item ID="27">
				<Name>Apparent energy import Un</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>VAh</Units>
				<Description>Apparent energy import, unified rate, on the 3 lines.</Description>
			</Item>
			<Item ID="28">
				<Name>Apparent energy export Un</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>VAh</Units>
				<Description>Apparent energy export, unified rate, on the 3 lines.</Description>
			</Item>
			<Item ID="29">
				<Name>Number of power failures in any phase</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>Number of power failures in any phase</Description>
			</Item>
			<Item ID="30">
				<Name>Number of long power failures in any phase</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>Number of long power failures in any phase.</Description>
			</Item>
			<Item ID="31">
				<Name>Time threshold for long power failure</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>s</Units>
				<Description>Time threshold for long power failure.</Description>
			</Item>
			<Item ID="32">
				<Name>Duration of last long power failure in any phase</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>s</Units>
				<Description>Duration of last long power failure in any phase.</Description>
			</Item>
			<Item ID="33">
				<Name>Threshold for missing voltage (voltage cut)</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration />
				<Units>V</Units>
				<Description>Threshold for missing voltage (voltage cut).</Description>
			</Item>
			<Item ID="34">
				<Name>Time threshold for voltage cut</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>s</Units>
				<Description>Time threshold for voltage cut.</Description>
			</Item>
			<Item ID="35">
				<Name>Voltage cut</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Boolean</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>Set to True if a voltage cut for a time above the time threshold and for a voltage below the voltage threshold is detected.</Description>
			</Item>
		</Resources>
		<Description2></Description2>
	</Object>
</LWM2M>
